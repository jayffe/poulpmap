{
  "name": "poulpmap",
  "version": "1.0.25",
  "description": "A interactive D3js map for creating stories compatible with the PoulpStudio project",
  "main": "./index.js",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist && npm run copy",
    "copy": "cp -rf src/PoulpMap.css dist && cp -rf package.json dist && cp -rf README.md dist",
    "send": "npm version patch && npm run copy && cd dist && npm publish"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/jayffe/poulpmap.git"
  },
  "babel": {
    "comments": false,
    "presets": [
      "@babel/preset-env",
      [
        "minify",
        {
          "builtIns": false,
          "evaluate": false,
          "mangle": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-preset-minify": "^0.5.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "d3-array": "^1.2.4",
    "d3-drag": "^1.2.3",
    "d3-ease": "^1.0.5",
    "d3-hierarchy": "^1.1.8",
    "d3-line": "^0.0.1",
    "d3-selection": "^1.3.2",
    "d3-shape": "^1.2.2",
    "d3-zoom": "^1.7.3"
  }
}
